<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="anchorPane" prefHeight="400.0" prefWidth="700.0" style="-fx-background-color: white;"
            fx:controller="unsw.controller.GameOverController"
            xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1">
    <Text fx:id="gameOver" layoutX="230.0" layoutY="149.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Game Over">
        <font>
            <Font name="System Bold Italic" size="44.0"/>
        </font>
    </Text>
    <Button fx:id="backButton" layoutX="260.0" layoutY="250.0" mnemonicParsing="false" onAction="#backToMainMenu"
            prefHeight="60.0" prefWidth="200.0" text="Back To Main Menu"/>
</AnchorPane>
